<body>

	<h3>New Concert</h3>

		<%= form_for @concert do |f| %>
			<%= f.label :artist %>:
			<%= f.text_field :artist %>
			<br>
			<%= f.label :venue %>:
			<%= f.text_field :venue %>
			<br>
			<%= f.label :city %>:
			<%= f.text_field :city %>
			<br>
			<%= f.label :date %>:
			<%= f.datetime_field :date %>
			<br>
			<%= f.label :price %>:
			<%= f.text_field :price %>
			<br>
			<%= f.label :description %>:
			<%= f.text_field :description %>
			<br>
			<%= f.submit %>
		<% end %>

</body>